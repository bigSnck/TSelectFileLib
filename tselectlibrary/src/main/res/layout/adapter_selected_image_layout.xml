<?xml version="1.0" encoding="utf-8"?>
<com.yt.tselectlibrary.ui.widget.SquareRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:clickable="false"
    android:background="@color/t_commom_text_withe"

    android:layout_height="wrap_content">

    <com.yt.tselectlibrary.ui.widget.SquareImageView
        android:layout_margin="2.5dp"
        android:id="@+id/item_siv_image"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerInside"
     />

    <com.yt.tselectlibrary.ui.widget.CheckView
        android:id="@+id/item_check_view"
        android:clickable="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="top|right"/>

</com.yt.tselectlibrary.ui.widget.SquareRelativeLayout>